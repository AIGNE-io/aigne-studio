{
  "name": "ai-studio",
  "private": true,
  "version": "0.4.277",
  "workspaces": [
    "blocklets/*",
    "packages/*",
    "framework/*"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "pnpm -F ai-studio dev",
    "bundle": "pnpm build:libs && pnpm bundle:ai-studio && pnpm bundle:ai-runtime",
    "bundle:ai-studio": "pnpm -F ai-studio bundle",
    "bundle:ai-runtime": "pnpm -F ai-runtime bundle",
    "lint": "pnpm --workspace-concurrency=2 -r run lint",
    "lint:fix": "pnpm --workspace-concurrency=2 -r run lint:fix",
    "build": "pnpm build:libs && pnpm -F ai-studio build",
    "clean": "pnpm -r run clean",
    "clean:deps": "rimraf ./**/node_modules",
    "build:libs": "pnpm --workspace-concurrency=2 -F @blocklet/* build",
    "bump-version": "zx scripts/bump-version.mjs",
    "update:deps": "npx -y taze latest -r -w -n '/blocklet|abtnode|arcblock|ocap|did-space|nft-studio|aigne/' && pnpm install && pnpm dedupe",
    "test": "pnpm -r run test",
    "e2e": "CI=true npx playwright test",
    "e2e:ui": "NODE_ENV=development npx playwright test --ui",
    "postinstall": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,mts,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx, .mts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@arcblock/did": "^1.21.0",
    "@arcblock/did-ext": "^1.21.0",
    "@arcblock/eslint-config": "^0.3.3",
    "@arcblock/eslint-config-ts": "^0.3.3",
    "@blocklet/ai-runtime": "workspace:^",
    "@blocklet/testlab": "^0.0.21",
    "@ocap/util": "^1.21.0",
    "@playwright/test": "^1.46.1",
    "bumpp": "^9.5.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^8.1.8",
    "lint-staged": "^15.2.9",
    "node-gyp-build": "~4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "start-server-and-test": "^2.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "zx": "^8.1.4"
  },
  "resolutions": {
    "@abtnode/client": "^1.16.46",
    "@abtnode/constant": "^1.16.46",
    "@abtnode/cron": "^1.16.46",
    "@abtnode/file-stream-rotator": "^0.6.3",
    "@arcblock/did": "^1.21.0",
    "@arcblock/did-auth": "^1.21.0",
    "@arcblock/did-connect": "^3.0.33",
    "@arcblock/did-ext": "^1.21.0",
    "@arcblock/did-util": "^1.21.0",
    "@arcblock/jwt": "^1.21.0",
    "@arcblock/ux": "^3.0.33",
    "@arcblock/ws": "^1.21.0",
    "@blocklet/ai-runtime": "workspace:^",
    "@blocklet/constant": "^1.16.46",
    "@blocklet/discuss-kit": "2.4.24",
    "@blocklet/discuss-kit-ux": "2.4.24",
    "@blocklet/editor": "2.4.24",
    "@blocklet/js-sdk": "^1.16.46",
    "@blocklet/logger": "^1.16.46",
    "@blocklet/meta": "^1.16.46",
    "@blocklet/sdk": "^1.16.46",
    "@blocklet/ui-react": "^3.0.33",
    "@blocklet/uploader": "^0.2.4",
    "@emotion/cache": "^11.14.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@nedb/core": "^2.1.5",
    "@ocap/asset": "^1.21.0",
    "@ocap/mcrypto": "^1.21.0",
    "@ocap/proto": "^1.21.0",
    "@ocap/types": "^1.21.0",
    "@ocap/util": "^1.21.0",
    "@ocap/wallet": "^1.21.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.7.5",
    "gpt-tokens": "1.2.0",
    "isomorphic-git": "1.27.1",
    "json-stable-stringify": "1.3.0",
    "monaco-editor": "0.50.0",
    "openapi3-ts": "4.3.1",
    "queue": "6.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "umzug": "^3.8.1",
    "vite-tsconfig-paths": "5.1.4",
    "ws": "^8.18.0"
  }
}
