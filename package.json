{
  "name": "ai-studio",
  "private": true,
  "version": "0.4.90",
  "workspaces": [
    "blocklets/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "pnpm -F ai-studio dev",
    "bundle": "pnpm build:libs && pnpm -F ai-studio -F ai-runtime bundle",
    "lint": "NODE_OPTIONS='--max_old_space_size=6144' pnpm build:libs && pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "build": "pnpm build:libs && pnpm -F ai-studio build",
    "clean": "pnpm -r run clean",
    "clean:deps": "rimraf ./**/node_modules",
    "build:libs": "pnpm -F @blocklet/* build",
    "bump-version": "zx scripts/bump-version.mjs",
    "update:deps": "npx -y taze major -r -w -x '/nanoid|eslint/' && pnpm install",
    "update:deps:internal": "npx -y taze major -r -w -n '/blocklet|abtnode|arcblock|ocap|did-space|nft-studio/' && pnpm install",
    "e2e": "./scripts/start-e2e.ts",
    "postinstall": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,mts,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx, .mts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.3.2",
    "@arcblock/eslint-config-ts": "^0.3.2",
    "@blocklet/ai-runtime": "workspace:^",
    "@blocklet/testlab": "^0.0.17",
    "@playwright/test": "^1.46.1",
    "bumpp": "^9.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^8.1.8",
    "lint-staged": "^15.2.9",
    "node-gyp-build": "~4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "start-server-and-test": "^2.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "zx": "^8.1.4"
  },
  "resolutions": {
    "@abtnode/client": "1.16.33-beta-20241015-065257-56072e40",
    "@abtnode/constant": "1.16.33-beta-20241015-065257-56072e40",
    "@abtnode/cron": "1.16.33-beta-20241015-065257-56072e40",
    "@abtnode/file-stream-rotator": "^0.6.3",
    "@arcblock/did": "^1.18.135",
    "@arcblock/did-auth": "^1.18.135",
    "@arcblock/did-connect": "^2.10.48",
    "@arcblock/did-ext": "^1.18.135",
    "@arcblock/did-util": "^1.18.135",
    "@arcblock/jwt": "^1.18.135",
    "@arcblock/ux": "^2.10.48",
    "@arcblock/ws": "^1.18.135",
    "@blocklet/js-sdk": "1.16.33-beta-20241015-065257-56072e40",
    "@blocklet/logger": "1.16.33-beta-20241015-065257-56072e40",
    "@blocklet/meta": "1.16.33-beta-20241015-065257-56072e40",
    "@blocklet/sdk": "1.16.33-beta-20241015-065257-56072e40",
    "@blocklet/ui-react": "^2.10.48",
    "@blocklet/uploader": "^0.1.45",
    "@emotion/cache": "^11.13.1",
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.3",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@nedb/core": "^2.1.5",
    "@ocap/asset": "^1.18.135",
    "@ocap/mcrypto": "^1.18.135",
    "@ocap/proto": "^1.18.135",
    "@ocap/types": "^1.18.135",
    "@ocap/util": "^1.18.135",
    "@ocap/wallet": "^1.18.135",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.5",
    "gpt-tokens": "1.2.0",
    "isomorphic-git": "1.25.7",
    "monaco-editor": "0.50.0",
    "openapi3-ts": "4.3.1",
    "queue": "6.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "vite-tsconfig-paths": "4.3.2",
    "ws": "^8.18.0"
  }
}
