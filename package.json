{
  "name": "ai-studio",
  "private": true,
  "version": "0.3.23",
  "workspaces": [
    "blocklets/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "pnpm -F ai-studio dev",
    "bundle": "pnpm build:libs && pnpm -F ai-studio -F ai-runtime bundle",
    "lint": "NODE_OPTIONS='--max_old_space_size=6144' pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "build": "pnpm build:libs && pnpm -F ai-studio build",
    "clean": "pnpm -r run clean",
    "clean:deps": "rimraf ./**/node_modules",
    "build:libs": "pnpm -F @blocklet/* build",
    "prepare": "husky install",
    "bump-version": "zx scripts/bump-version.mjs",
    "update:deps": "npx -y taze -r -w && pnpm install",
    "e2e": "./scripts/start-e2e.ts"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,mts,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx, .mts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.4",
    "@arcblock/eslint-config-ts": "^0.2.4",
    "@blocklet/testlab": "^0.0.9",
    "@playwright/test": "^1.45.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^7.4.2",
    "lint-staged": "^12.5.0",
    "node-gyp-build": "~4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^2.0.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "zx": "^8.1.4"
  },
  "resolutions": {
    "@abtnode/client": "1.16.28",
    "@abtnode/constant": "1.16.28",
    "@abtnode/cron": "1.16.28",
    "@abtnode/file-stream-rotator": "^0.6.3",
    "@arcblock/did": "^1.18.126",
    "@arcblock/did-auth": "^1.18.126",
    "@arcblock/did-connect": "^2.10.3",
    "@arcblock/did-ext": "^1.18.126",
    "@arcblock/did-util": "^1.18.126",
    "@arcblock/jwt": "^1.18.126",
    "@arcblock/ux": "^2.10.3",
    "@arcblock/ws": "^1.18.126",
    "@blocklet/js-sdk": "1.16.28",
    "@blocklet/logger": "1.16.28",
    "@blocklet/meta": "1.16.28",
    "@blocklet/sdk": "1.16.28",
    "@blocklet/ui-react": "^2.10.3",
    "@blocklet/uploader": "^0.0.73",
    "@emotion/cache": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^5.16.0",
    "@mui/lab": "^5.0.0-alpha.171",
    "@mui/material": "^5.16.0",
    "@nedb/core": "^2.1.5",
    "@ocap/asset": "^1.18.126",
    "@ocap/mcrypto": "^1.18.126",
    "@ocap/proto": "^1.18.126",
    "@ocap/types": "^1.18.126",
    "@ocap/util": "^1.18.126",
    "@ocap/wallet": "^1.18.126",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.2",
    "gpt-tokens": "1.2.0",
    "isomorphic-git": "1.25.7",
    "openapi3-ts": "4.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1"
  }
}
