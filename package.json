{
  "name": "ai-studio",
  "private": true,
  "version": "0.1.284",
  "workspaces": [
    "blocklets/*",
    "packages/*"
  ],
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "yarn workspace ai-studio dev",
    "bundle": "npm run build:libs && yarn workspace ai-studio bundle",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "build": "npm run build:libs && yarn workspace ai-studio build",
    "clean": "yarn workspaces run clean",
    "build:libs": "yarn workspace @blocklet/co-git build && yarn workspace @blocklet/studio-ui build && yarn workspace @blocklet/prompt-editor build && yarn workspace @blocklet/ai-runtime build && yarn workspace @blocklet/dataset-sdk build && yarn workspace @blocklet/ai-builtin build",
    "prepare": "husky install",
    "bump-version": "zx scripts/bump-version.mjs",
    "update:deps": "npx -y taze -r -w && yarn install && npx -y yarn-deduplicate yarn.lock && yarn"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.4",
    "@arcblock/eslint-config-ts": "^0.2.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^7.4.2",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^3.0.2",
    "typescript": "^5.4.2",
    "zx": "^7.2.3"
  },
  "resolutions": {
    "@abtnode/client": "^1.16.24",
    "@abtnode/constant": "^1.16.24",
    "@arcblock/did": "^1.18.113",
    "@arcblock/did-auth": "^1.18.113",
    "@arcblock/did-ext": "^1.18.113",
    "@arcblock/did-util": "^1.18.113",
    "@arcblock/jwt": "^1.18.113",
    "@arcblock/ws": "^1.18.113",
    "@blocklet/meta": "^1.16.24",
    "@blocklet/sdk": "^1.16.24",
    "@blocklet/uploader": "^0.0.73",
    "@mui/material": "^5.15.13",
    "@nedb/core": "^2.1.5",
    "@ocap/asset": "^1.18.113",
    "@ocap/mcrypto": "^1.18.113",
    "@ocap/proto": "^1.18.113",
    "@ocap/types": "^1.18.113",
    "@ocap/util": "^1.18.113",
    "@ocap/wallet": "^1.18.113",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "axios": "^1.6.7",
    "gpt-tokens": "1.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-router-dom": "6.16.0"
  }
}
