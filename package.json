{
  "name": "ai-studio",
  "private": true,
  "version": "0.1.238",
  "workspaces": [
    "blocklets/*",
    "packages/*"
  ],
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "yarn workspace ai-studio dev",
    "bundle": "npm run build:libs && yarn workspace ai-studio bundle",
    "lint": "yarn workspaces run lint",
    "build": "npm run build:libs && yarn workspace ai-studio build",
    "clean": "yarn workspaces run clean",
    "build:libs": "yarn workspace @blocklet/co-git build && yarn workspace @blocklet/studio-ui build && yarn workspace @blocklet/prompt-editor build && yarn workspace @blocklet/ai-runtime build",
    "prepare": "husky install",
    "bump-version": "zx scripts/bump-version.mjs",
    "update:deps": "npx -y taze -r -w && yarn install && npx -y yarn-deduplicate yarn.lock && yarn"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.4",
    "@arcblock/eslint-config-ts": "^0.2.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^7.4.2",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "rimraf": "^3.0.2",
    "typescript": "^5.3.3",
    "zx": "^7.2.3"
  },
  "resolutions": {
    "@arcblock/did": "^1.18.108",
    "@arcblock/did-auth": "^1.18.108",
    "@arcblock/did-ext": "^1.18.108",
    "@arcblock/did-util": "^1.18.108",
    "@arcblock/ws": "^1.18.108",
    "@blocklet/constant": "1.16.22-beta-079a9053",
    "@lexical/clipboard": "0.12.5",
    "@lexical/code": "0.12.5",
    "@lexical/file": "0.12.5",
    "@lexical/hashtag": "0.12.5",
    "@lexical/headless": "0.12.5",
    "@lexical/link": "0.12.5",
    "@lexical/list": "0.12.5",
    "@lexical/mark": "0.12.5",
    "@lexical/overflow": "0.12.5",
    "@lexical/plain-text": "0.12.5",
    "@lexical/react": "0.12.5",
    "@lexical/selection": "0.12.5",
    "@lexical/utils": "0.12.5",
    "@mui/material": "^5.15.6",
    "@nedb/core": "^2.1.5",
    "@ocap/asset": "^1.18.108",
    "@ocap/proto": "^1.18.108",
    "@ocap/types": "^1.18.108",
    "@ocap/util": "^1.18.108",
    "@ocap/wallet": "^1.18.108",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.7",
    "lexical": "0.12.5",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-router-dom": "6.16.0"
  }
}
