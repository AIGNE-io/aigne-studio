{
  "name": "ai-studio",
  "version": "0.1.40",
  "scripts": {
    "dev": "blocklet dev",
    "eject": "vite eject",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development nodemon api/dev.ts -w api",
    "clean": "node scripts/build-clean.js",
    "bundle": "tsc --noEmit && npm run bundle:client && npm run bundle:api",
    "bundle:client": "vite build",
    "bundle:api": "npm run clean && tsc -p tsconfig.api.json && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepare": "husky install",
    "update:deps": "npx taze -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && yarn && npm run deduplicate",
    "deduplicate": "npx yarn-deduplicate yarn.lock",
    "bump-version": "zx --quiet scripts/bump-version.mjs"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcblock/did-auth": "^1.18.78",
    "@arcblock/did-auth-storage-nedb": "^1.7.1",
    "@arcblock/did-connect": "^2.5.37",
    "@arcblock/ux": "^2.5.37",
    "@beskar-labs/gpt-encoder": "^1.0.6",
    "@blocklet/ai-kit": "^0.0.55",
    "@blocklet/sdk": "^1.16.7",
    "@blocklet/ui-react": "^2.5.37",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@iconify-icon/react": "^1.0.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.125",
    "@mui/material": "^5.11.12",
    "@mui/x-date-pickers": "^6.0.1",
    "@nedb/core": "^2.1.5",
    "@ocap/client": "^1.18.78",
    "@ocap/mcrypto": "^1.18.78",
    "@ocap/wallet": "^1.18.78",
    "ahooks": "^3.7.4",
    "axios": "^1.3.2",
    "circular-natal-horoscope-js": "^1.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "flat": "^5.0.2",
    "framer-motion": "^10.12.2",
    "immer": "^9.0.19",
    "joi": "^17.7.1",
    "json-stable-stringify": "^1.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^0.0.39",
    "react-router-dom": "^6.8.0",
    "rimraf": "^3.0.2",
    "tz-lookup": "^6.1.25",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.3",
    "@arcblock/eslint-config-ts": "^0.2.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.15",
    "@types/file-saver": "^2.0.5",
    "@types/flat": "^5.0.2",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/tz-lookup": "^6.1.0",
    "@vitejs/plugin-react": "^2.2.0",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-blocklet": "^0.6.2",
    "vite-plugin-svgr": "^2.4.0",
    "zx": "^7.1.1"
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "resolutions": {
    "@nedb/core": "^2.1.5",
    "@mui/material": "^5.11.11"
  }
}
