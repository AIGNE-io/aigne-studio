{
  "name": "@aigne/core",
  "version": "0.4.204",
  "description": "AIGNE core library",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --outDir lib/cjs",
    "build:esm": "tsc -p tsconfig.json --module es2022 --outDir lib/esm",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --outDir lib/types",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "test": "jest src/"
  },
  "dependencies": {
    "@abtnode/client": "^1.16.37",
    "@blocklet/dataset-sdk": "^0.4.204",
    "@blocklet/sdk": "^1.16.37",
    "@types/mustache": "^4.2.5",
    "axios": "^1.7.5",
    "cron": "^3.1.7",
    "fast-deep-equal": "^3.1.3",
    "fastq": "^1.17.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "joi": "^17.13.3",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "json-logic-js": "^2.0.5",
    "json-stable-stringify": "^1.1.1",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "nanoid": "^3.3.7",
    "openapi3-ts": "^4.3.3",
    "react-querybuilder": "^7.7.1",
    "snowflake-uuid": "^1.0.0",
    "tsyringe": "^4.8.0",
    "ufo": "^1.5.4",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
