{
  "name": "@aigne/runtime",
  "version": "0.4.209",
  "description": "AIGNE core library",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.js",
  "bin": {
    "aigne-cli": "./lib/cjs/cmd/aigne-cli.js"
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "./*.js",
    "./*.cjs",
    "./*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir lib/cjs",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --outDir lib/esm",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir lib/types",
    "clean": "rimraf lib",
    "test": "bun test",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./client": {
      "import": "./client.js",
      "require": "./client.cjs",
      "types": "./client.d.ts"
    },
    "./middleware": {
      "import": "./middleware.js",
      "require": "./middleware.cjs",
      "types": "./middleware.d.ts"
    },
    "./cmd": {
      "import": "./cmd.js",
      "require": "./cmd.cjs",
      "types": "./cmd.d.ts"
    }
  },
  "dependencies": {
    "@aigne/agent-v1": "workspace:^",
    "@aigne/core": "workspace:^",
    "@blocklet/ai-kit": "^0.1.66",
    "@blocklet/js-sdk": "^1.16.37",
    "@blocklet/quickjs": "workspace:^",
    "@blocklet/sdk": "^1.16.37",
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "eventsource-parser": "^2.0.1",
    "express": "^4.19.2",
    "glob": "^10.4.5",
    "immer": "^10.1.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.5.4",
    "ufo": "^1.5.4",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@aryzing/bun-mock-fetch": "^0.2.2",
    "@types/bun": "^1.1.16",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  }
}
