{
  "name": "@aigne/memory",
  "version": "0.4.195",
  "description": "AIGNE Memory library",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.js",
  "bin": {
    "aigne-cli": "./lib/cjs/cmd/aigne-cli.js"
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "./*.js",
    "./*.cjs",
    "./*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir lib/cjs",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --outDir lib/esm",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir lib/types",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "test": "jest src/"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "dependencies": {
    "@aigne/core": "workspace:^",
    "@blocklet/ai-kit": "^0.1.66",
    "@blocklet/constant": "^1.16.36",
    "@blocklet/sdk": "1.16.36",
    "@blocklet/search-kit-js": "^0.2.18",
    "@langchain/community": "^0.3.15",
    "@langchain/core": "^0.3.18",
    "axios": "^1.7.5",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "express": "^4.19.2",
    "faiss-node": "^0.5.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "lru-cache": "^10.4.3",
    "sequelize": "^6.37.5",
    "snowflake-uuid": "^1.0.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.5.4",
    "ufo": "^1.5.4",
    "umzug": "^3.8.1",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5"
  }
}
