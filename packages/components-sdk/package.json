{
  "name": "@blocklet/components-sdk",
  "version": "0.2.33",
  "description": "The SDK for processing react component protocol",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./const": {
      "import": "./lib/esm/const.js",
      "require": "./lib/cjs/const.js"
    },
    "./type": {
      "import": "./lib/esm/type.js",
      "require": "./lib/cjs/type.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "./const": [
        "./lib/types/const.d.ts"
      ],
      "./type": [
        "./lib/types/type.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module es2022 --outDir lib/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@blocklet/constant": "^1.16.26",
    "@blocklet/sdk": "^1.16.26",
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "ufo": "^1.5.3",
    "yaml": "^2.4.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
