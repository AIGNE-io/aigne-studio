{
  "name": "@blocklet/studio-ui",
  "version": "0.5.0",
  "description": "UI components for studio",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --incremental false --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc --module es2022 --incremental false --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc --declaration --emitDeclarationOnly --incremental false --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@arcblock/did-connect": "^3.0.33",
    "@arcblock/react-hooks": "^3.0.33",
    "@arcblock/ux": "^3.0.33",
    "@blocklet/ui-react": "^3.0.33",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@arcblock/did-connect": "^3.0.33",
    "@arcblock/ux": "^3.0.33",
    "@blocklet/ui-react": "^3.0.33",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@types/react": "^19.1.8",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.0",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  }
}
