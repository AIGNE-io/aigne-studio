{
  "name": "@blocklet/studio-ui",
  "version": "0.4.20",
  "description": "UI components for studio",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module es2022 --outDir lib/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@arcblock/did-connect": "^2.9.51",
    "@arcblock/ux": "^2.9.51",
    "@blocklet/ui-react": "^2.9.51",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.11",
    "react": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@arcblock/did-connect": "^2.10.12",
    "@arcblock/ux": "^2.10.12",
    "@blocklet/ui-react": "^2.10.12",
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.0",
    "@mui/icons-material": "^5.16.6",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.6",
    "@types/react": "^18.3.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  }
}
