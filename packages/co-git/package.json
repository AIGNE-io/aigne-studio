{
  "name": "@blocklet/co-git",
  "version": "0.4.267",
  "description": "The cooperative component library for AI Studio",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./repository": {
      "import": "./lib/esm/repository/index.js",
      "require": "./lib/cjs/repository/index.js",
      "types": "./lib/types/repository/index.d.ts"
    },
    "./yjs": {
      "import": "./lib/esm/yjs.js",
      "require": "./lib/cjs/yjs.js",
      "types": "./lib/types/yjs.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "repository": [
        "./lib/types/repository/index.d.ts"
      ],
      "yjs": [
        "./lib/types/yjs.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --incremental false --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc --module es2022 --incremental false --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc --declaration --emitDeclarationOnly --incremental false --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@syncedstore/core": "^0.6.0",
    "@syncedstore/react": "^0.6.0",
    "fs-extra": "^11.2.0",
    "isomorphic-git": "^1.29.0",
    "lib0": "^0.2.97",
    "lodash": "^4.17.21",
    "queue": "^7.0.0",
    "ws": "^8.18.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.18"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  }
}
