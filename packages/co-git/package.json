{
  "name": "@blocklet/co-git",
  "version": "0.0.1",
  "description": "The cooperative component library for AI Studio",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./repository": {
      "import": "./lib/esm/repository/index.js",
      "require": "./lib/cjs/repository/index.js"
    },
    "./yjs": {
      "import": "./lib/esm/yjs.js",
      "require": "./lib/cjs/yjs.js"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "build": "tsc --module commonjs --outDir lib/cjs && tsc --module es2022 --outDir lib/esm  && tsc --declaration --emitDeclarationOnly --outDir .",
    "build:watch": "tsc --module commonjs --outDir lib/cjs -w & tsc --module es2022 --outDir lib/esm -w & tsc --declaration --emitDeclarationOnly --outDir . -w",
    "dev": "npm run storybook & npm run watch",
    "clean": "rimraf lib",
    "start": "npm run storybook",
    "prepublishOnly": "npm run build",
    "preview": "npm run storybook",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "update:deps": "npx taze -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && npm run deduplicate && yarn",
    "deduplicate": "npx yarn-deduplicate ../../yarn.lock"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@syncedstore/core": "^0.6.0-alpha.0",
    "@syncedstore/react": "^0.6.0-alpha.0",
    "isomorphic-git": "^1.24.5",
    "lib0": "^0.2.85",
    "lodash": "^4.17.21",
    "queue": "^6.0.2",
    "ws": "^8.14.1",
    "y-protocols": "^1.0.6",
    "yaml": "^2.3.2",
    "yjs": "^13.6.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.198",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.4"
  }
}
