{
  "name": "@blocklet/prompt-editor",
  "version": "0.1.238",
  "description": "prompt editor for ai studio",
  "author": "arcblock",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./utils": {
      "import": "./lib/esm/utils.js",
      "require": "./lib/cjs/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "utils": [
        "./lib/types/utils.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "bugs": {
    "url": "https://github.com/blocklet/ai-studio/issues"
  },
  "scripts": {
    "start": "vite --host",
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx --fix",
    "clean": "rimraf lib",
    "build": "npm run clean && run-p build:*",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module es2022 --outDir lib/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "build:css-cjs": "copyfiles -u 1 './src/**/*.css' lib/cjs/",
    "build:css-esm": "copyfiles -u 1 './src/**/*.css' lib/esm/",
    "dev": "run-p 'build:* -- -w'",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@lexical/clipboard": "^0.12.6",
    "@lexical/code": "^0.12.6",
    "@lexical/file": "^0.12.6",
    "@lexical/hashtag": "^0.12.6",
    "@lexical/headless": "^0.12.6",
    "@lexical/link": "^0.12.6",
    "@lexical/list": "^0.12.6",
    "@lexical/mark": "^0.12.6",
    "@lexical/overflow": "^0.12.6",
    "@lexical/plain-text": "^0.12.6",
    "@lexical/react": "^0.12.6",
    "@lexical/selection": "^0.12.6",
    "@lexical/utils": "^0.12.6",
    "@mui/material": "^5.15.6",
    "ahooks": "^3.7.9",
    "lexical": "^0.12.6",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "y-websocket": "^1.5.3",
    "yjs": "^13.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "5.3.0",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-react": "^4.2.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5",
    "vite": "^5.0.12",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-svgr": "^4.2.0"
  }
}
