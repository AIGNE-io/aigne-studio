{
  "name": "@blocklet/prompt-editor",
  "version": "0.4.91",
  "description": "prompt editor for ai studio",
  "author": "arcblock",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./utils": {
      "import": "./lib/esm/utils.js",
      "require": "./lib/cjs/utils.js",
      "types": "./lib/types/utils.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "utils": [
        "./lib/types/utils.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "bugs": {
    "url": "https://github.com/blocklet/ai-studio/issues"
  },
  "scripts": {
    "start": "vite --host",
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf lib",
    "build": "npm run clean && run-p build:*",
    "build:cjs": "tsc --module commonjs --incremental false --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc --module es2022 --incremental false --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc --declaration --emitDeclarationOnly --incremental false --outDir lib/types > /dev/null || exit 0",
    "build:css-cjs": "copyfiles -u 1 './src/**/*.css' lib/cjs/",
    "build:css-esm": "copyfiles -u 1 './src/**/*.css' lib/esm/",
    "dev": "run-p 'build:* -- -w'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@lexical/clipboard": "0.13.1",
    "@lexical/code": "0.13.1",
    "@lexical/file": "0.13.1",
    "@lexical/hashtag": "0.13.1",
    "@lexical/headless": "0.13.1",
    "@lexical/history": "0.13.1",
    "@lexical/link": "0.13.1",
    "@lexical/list": "0.13.1",
    "@lexical/mark": "0.13.1",
    "@lexical/overflow": "0.13.1",
    "@lexical/plain-text": "0.13.1",
    "@lexical/react": "0.13.1",
    "@lexical/selection": "0.13.1",
    "@lexical/utils": "0.13.1",
    "@mui/material": "^5.16.7",
    "ahooks": "^3.8.1",
    "lexical": "0.13.1",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.18"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "6.0.4",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-react": "^4.3.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "vite": "^5.4.2",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-svgr": "^4.2.0"
  }
}
