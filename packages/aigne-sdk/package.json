{
  "name": "@blocklet/aigne-sdk",
  "version": "0.5.0",
  "description": "AIGNE SDK used to interact with AIGNE services",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./api/*": {
      "import": "./lib/esm/api/*.js",
      "require": "./lib/cjs/api/*.js",
      "types": "./lib/types/api/*.d.ts"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js",
      "types": "./lib/types/components/index.d.ts"
    },
    "./components/*": {
      "import": "./lib/esm/components/*/index.js",
      "require": "./lib/cjs/components/*/index.js",
      "types": "./lib/types/components/*/index.d.ts"
    },
    "./server/*": {
      "import": "./lib/esm/server/*.js",
      "require": "./lib/cjs/server/*.js",
      "types": "./lib/types/server/*.d.ts"
    },
    "./utils/*": {
      "import": "./lib/esm/utils/*.js",
      "require": "./lib/cjs/utils/*.js",
      "types": "./lib/types/utils/*.d.ts"
    },
    "./constants": {
      "import": "./lib/esm/constants.js",
      "require": "./lib/cjs/constants.js",
      "types": "./lib/types/constants.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "components/*": [
        "./lib/types/components/*/index.d.ts"
      ],
      "server/*": [
        "./lib/types/server/*.d.ts"
      ],
      "utils/*": [
        "./lib/types/utils/*.d.ts"
      ],
      "constants": [
        "./lib/types/constants.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --incremental false --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --incremental false --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --incremental false --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@blocklet/ai-runtime": "workspace:^",
    "@blocklet/aigne-hub": "^0.3.3",
    "@blocklet/js-sdk": "^1.16.46",
    "@blocklet/pages-kit": "^0.6.34",
    "@blocklet/sdk": "^1.16.46",
    "@microsoft/fetch-event-source": "^2.0.1",
    "ahooks": "^3.8.1",
    "axios": "^1.7.5",
    "dayjs": "^1.11.13",
    "eventsource-parser": "^2.0.1",
    "joi": "^17.13.3",
    "lru-cache": "^10.4.3",
    "nanoid": "^3.3.7",
    "react-hook-form": "^7.53.0",
    "ufo": "^1.5.4"
  },
  "peerDependencies": {
    "@arcblock/ux": "^3.0.33",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "react": "^19.1.0",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@arcblock/ux": "^3.0.39",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@jest/globals": "^29.7.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@types/lodash": "^4.17.7",
    "@types/mustache": "^4.2.5",
    "@types/react": "^19.1.8",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.0",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
