{
  "name": "@blocklet/ai-runtime",
  "version": "0.4.45",
  "description": "Runtime for AI Kits",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./api": {
      "import": "./lib/esm/api/index.js",
      "require": "./lib/cjs/api/index.js"
    },
    "./api/*": {
      "import": "./lib/esm/api/*.js",
      "require": "./lib/cjs/api/*.js"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js"
    },
    "./types": {
      "import": "./lib/esm/types/index.js",
      "require": "./lib/cjs/types/index.js"
    },
    "./types/*": {
      "import": "./lib/esm/types/*.js",
      "require": "./lib/cjs/types/*.js"
    },
    "./core": {
      "import": "./lib/esm/core/index.js",
      "require": "./lib/cjs/core/index.js"
    },
    "./core/utils/*": {
      "import": "./lib/esm/core/utils/*.js",
      "require": "./lib/cjs/core/utils/*.js"
    },
    "./common": {
      "import": "./lib/esm/common/index.js",
      "require": "./lib/cjs/common/index.js"
    },
    "./common/*": {
      "import": "./lib/esm/common/*.js",
      "require": "./lib/cjs/common/*.js"
    },
    "./utils/*": {
      "import": "./lib/esm/utils/*.js",
      "require": "./lib/cjs/utils/*.js"
    },
    "./constants": {
      "import": "./lib/esm/constants.js",
      "require": "./lib/cjs/constants.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api": [
        "./lib/types/api/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts",
        "./lib/types/api/*/index.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "types": [
        "./lib/types/types/index.d.ts"
      ],
      "types/runtime/*": [
        "./lib/types/types/runtime/*.d.ts"
      ],
      "core": [
        "./lib/types/core/index.d.ts"
      ],
      "core/utils/*": [
        "./lib/types/core/utils/*.d.ts"
      ],
      "common": [
        "./lib/types/common/index.d.ts"
      ],
      "common/*": [
        "./lib/types/common/*.d.ts"
      ],
      "utils/*": [
        "./lib/types/utils/*.d.ts"
      ],
      "constants": [
        "./lib/types/constants.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "test": "jest src/"
  },
  "dependencies": {
    "@blocklet/ai-builtin": "workspace:^",
    "@blocklet/ai-kit": "^0.1.47",
    "@blocklet/dataset-sdk": "workspace:^",
    "@blocklet/logger": "^1.16.31",
    "@blocklet/sdk": "^1.16.31",
    "@microsoft/fetch-event-source": "^2.0.1",
    "ahooks": "^3.8.1",
    "axios": "^1.7.5",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "eventsource-parser": "^2.0.1",
    "fast-deep-equal": "^3.1.3",
    "fastq": "^1.17.1",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.3",
    "js-base64": "^3.7.7",
    "json-stable-stringify": "^1.1.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "node-fetch": "2",
    "react-flags-select": "^2.2.3",
    "react-hook-form": "^7.53.0",
    "recoil": "^0.7.7",
    "snowflake-uuid": "^1.0.0",
    "typescript": "^5.5.4",
    "ufo": "^1.5.4",
    "vm2": "^3.9.19",
    "yaml": "^2.5.0"
  },
  "peerDependencies": {
    "@arcblock/ux": "^2.9.51",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.11",
    "react": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@arcblock/ux": "^2.10.30",
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.3",
    "@jest/globals": "^29.7.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@types/lodash": "^4.17.7",
    "@types/mustache": "^4.2.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
