{
  "name": "@blocklet/ai-runtime",
  "version": "0.1.340-beta.0",
  "description": "Runtime for AI Kits",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./api": {
      "import": "./lib/esm/api/index.js",
      "require": "./lib/cjs/api/index.js"
    },
    "./api/*": {
      "import": "./lib/esm/api/*.js",
      "require": "./lib/cjs/api/*.js"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js"
    },
    "./types": {
      "import": "./lib/esm/types/index.js",
      "require": "./lib/cjs/types/index.js"
    },
    "./core": {
      "import": "./lib/esm/core/index.js",
      "require": "./lib/cjs/core/index.js"
    },
    "./common": {
      "import": "./lib/esm/common.js",
      "require": "./lib/cjs/common.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api": [
        "./lib/types/api/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts",
        "./lib/types/api/*/index.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "types": [
        "./lib/types/types/index.d.ts"
      ],
      "core": [
        "./lib/types/core/index.d.ts"
      ],
      "common": [
        "./lib/types/common.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@blocklet/ai-builtin": "^0.1.340",
    "@blocklet/ai-kit": "^0.1.26",
    "@blocklet/dataset-sdk": "^0.1.340",
    "@blocklet/sdk": "^1.16.25",
    "@microsoft/fetch-event-source": "^2.0.1",
    "ahooks": "^3.7.11",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "eventsource-parser": "^1.1.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "node-fetch": "2",
    "react-hook-form": "^7.51.3",
    "recoil": "^0.7.7",
    "snowflake-uuid": "^1.0.0",
    "typescript": "^5.4.5",
    "ufo": "^1.5.3",
    "vm2": "^3.9.19"
  },
  "peerDependencies": {
    "@arcblock/ux": "^2.9.51",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.11",
    "react": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@arcblock/ux": "^2.9.69",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@types/lodash": "^4.17.0",
    "@types/mustache": "^4.2.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.79",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
