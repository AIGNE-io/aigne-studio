{
  "name": "@blocklet/ai-runtime",
  "version": "0.5.7",
  "description": "Runtime for AI Kits",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./api": {
      "import": "./lib/esm/api/index.js",
      "require": "./lib/cjs/api/index.js",
      "types": "./lib/types/api/index.d.ts"
    },
    "./api/*": {
      "import": "./lib/esm/api/*.js",
      "require": "./lib/cjs/api/*.js",
      "types": "./lib/types/api/*.d.ts"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js",
      "types": "./lib/types/components/index.d.ts"
    },
    "./types": {
      "import": "./lib/esm/types/index.js",
      "require": "./lib/cjs/types/index.js",
      "types": "./lib/types/types/index.d.ts"
    },
    "./types/*": {
      "import": "./lib/esm/types/*.js",
      "require": "./lib/cjs/types/*.js",
      "types": "./lib/types/types/*.d.ts"
    },
    "./core": {
      "import": "./lib/esm/core/index.js",
      "require": "./lib/cjs/core/index.js",
      "types": "./lib/types/core/index.d.ts"
    },
    "./core/utils/*": {
      "import": "./lib/esm/core/utils/*.js",
      "require": "./lib/cjs/core/utils/*.js",
      "types": "./lib/types/core/utils/*.d.ts"
    },
    "./common": {
      "import": "./lib/esm/common/index.js",
      "require": "./lib/cjs/common/index.js",
      "types": "./lib/types/common/index.d.ts"
    },
    "./common/*": {
      "import": "./lib/esm/common/*.js",
      "require": "./lib/cjs/common/*.js",
      "types": "./lib/types/common/*.d.ts"
    },
    "./utils/*": {
      "import": "./lib/esm/utils/*.js",
      "require": "./lib/cjs/utils/*.js",
      "types": "./lib/types/utils/*.d.ts"
    },
    "./constants": {
      "import": "./lib/esm/constants.js",
      "require": "./lib/cjs/constants.js",
      "types": "./lib/types/constants.d.ts"
    },
    "./front": {
      "import": "./lib/esm/front/index.js",
      "require": "./lib/cjs/front/index.js",
      "types": "./lib/types/front/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/index.d.ts"
      ],
      "api": [
        "./lib/types/api/index.d.ts"
      ],
      "api/*": [
        "./lib/types/api/*.d.ts",
        "./lib/types/api/*/index.d.ts"
      ],
      "components": [
        "./lib/types/components/index.d.ts"
      ],
      "types": [
        "./lib/types/types/index.d.ts"
      ],
      "types/runtime/*": [
        "./lib/types/types/runtime/*.d.ts"
      ],
      "core": [
        "./lib/types/core/index.d.ts"
      ],
      "core/utils/*": [
        "./lib/types/core/utils/*.d.ts"
      ],
      "common": [
        "./lib/types/common/index.d.ts"
      ],
      "common/*": [
        "./lib/types/common/*.d.ts"
      ],
      "utils/*": [
        "./lib/types/utils/*.d.ts"
      ],
      "constants": [
        "./lib/types/constants.d.ts"
      ],
      "front": [
        "./lib/types/front/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "src",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-studio.git"
  },
  "scripts": {
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc --noEmit && run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --incremental false --outDir lib/cjs > /dev/null || exit 0",
    "build:esm": "tsc -p tsconfig.build.json --module es2022 --incremental false --outDir lib/esm > /dev/null || exit 0",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --incremental false --outDir lib/types > /dev/null || exit 0",
    "dev": "run-p 'build:* -- -w'",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "test": "jest src/"
  },
  "dependencies": {
    "@abtnode/client": "^1.16.48",
    "@arcblock/did-connect-react": "^3.1.15",
    "@blocklet/aigne-hub": "^0.3.12",
    "@blocklet/co-git": "workspace:^",
    "@blocklet/constant": "^1.16.48",
    "@blocklet/dataset-sdk": "workspace:^",
    "@blocklet/js-sdk": "^1.16.48",
    "@blocklet/logger": "^1.16.48",
    "@blocklet/pages-kit": "^0.6.48",
    "@blocklet/quickjs": "workspace:^",
    "@blocklet/sdk": "^1.16.48",
    "@blocklet/uploader": "^0.2.7",
    "@iconify-icons/tabler": "^1.2.95",
    "@iconify/react": "^5.0.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/react-scroll-to-bottom": "^4.2.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ahooks": "^3.8.1",
    "ajv": "^8.17.1",
    "axios": "^1.7.5",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "eventsource-parser": "^2.0.1",
    "express": "^4.19.2",
    "fast-deep-equal": "^3.1.3",
    "fastq": "^1.17.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.2.0",
    "html2pdf.js": "^0.10.2",
    "isomorphic-dompurify": "^2.16.0",
    "joi": "^17.13.3",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "json-logic-js": "^2.0.5",
    "json-stable-stringify": "^1.3.0",
    "jsonata": "^2.0.5",
    "lodash": "^4.17.21",
    "lru-cache": "^10.4.3",
    "material-ui-popup-state": "^5.1.2",
    "nanoid": "^3.3.7",
    "node-fetch": "2",
    "react-error-boundary": "^4.0.13",
    "react-flags-select": "^2.2.3",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.53.0",
    "react-markdown": "^10.0.1",
    "react-promise-suspense": "^0.3.4",
    "react-querybuilder": "^8.8.1",
    "react-scroll-to-bottom": "^4.2.0",
    "react-share": "^5.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-transitioning": "^1.0.7",
    "react-wrap-balancer": "^1.1.1",
    "remark-gfm": "^4.0.0",
    "semver": "^7.6.3",
    "snowflake-uuid": "^1.0.0",
    "ufo": "^1.5.4",
    "yaml": "^2.5.0",
    "zustand": "^4.5.5"
  },
  "peerDependencies": {
    "@arcblock/ux": "^3.0.33",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "react": "^19.1.0",
    "react-router-dom": "^6.26.1",
    "@blocklet/pages-kit": "^0.6.46"
  },
  "devDependencies": {
    "@arcblock/ux": "^3.1.15",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@jest/globals": "^29.7.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@types/json-logic-js": "^2.0.7",
    "@types/lodash": "^4.17.7",
    "@types/mustache": "^4.2.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^19.1.8",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.0",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
