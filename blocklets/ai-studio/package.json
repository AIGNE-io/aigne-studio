  "name": "ai-studio",
  "version": "0.1.186",
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "COMPONENT_STORE_URL=https://test.store.blocklet.dev blocklet dev",
    "eject": "vite eject",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development nodemon",
    "clean": "node scripts/build-clean.js",
    "build": "tsc --noEmit && npm run bundle:client && tsc -p tsconfig.api.json",
    "bundle": "tsc --noEmit && npm run bundle:client && npm run bundle:api",
    "bundle:client": "vite build",
    "bundle:api": "npm run clean && tsc -p tsconfig.api.json && blocklet bundle --zip --monorepo --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "update:deps": "npx taze -x @blocklet/hnswlib-node -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && npm run deduplicate  && yarn",
    "deduplicate": "npx yarn-deduplicate ../../yarn.lock"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
