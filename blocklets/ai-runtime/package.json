{
  "name": "ai-runtime",
  "version": "0.7.8",
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "cross-env COMPONENT_STORE_URL=https://test.store.blocklet.dev blocklet dev",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "vite-node -c vite-server.config.ts -w api/dev.ts",
    "clean": "node scripts/build-clean.js",
    "build": "run-s clean bundle:client bundle:api",
    "bundle": "run-s clean bundle:*",
    "bundle:client": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "bundle:api": "tsc -p tsconfig.api.json && tsc-alias -p tsconfig.api.json",
    "bundle:blocklet": "blocklet bundle --compact --external sqlite3,faiss-node,pdf2json,quickjs-emscripten --monorepo --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
