{
  "name": "ai-runtime",
  "version": "0.1.0",
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "scripts": {
    "dev": "cross-env COMPONENT_STORE_URL=https://test.store.blocklet.dev blocklet dev",
    "lint": "tsc --noEmit && eslint src api/src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development nodemon",
    "clean": "node scripts/build-clean.js",
    "build": "run-s clean bundle:client bundle:api",
    "bundle": "run-s clean bundle:*",
    "bundle:client": "vite build",
    "bundle:api": "tsc -p tsconfig.api.json && tsc-alias -p tsconfig.api.json",
    "bundle:blocklet": "blocklet bundle --compact --external sqlite3,faiss-node,pdf2json --monorepo --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcblock/did-auth": "^1.18.123",
    "@arcblock/did-auth-storage-nedb": "^1.7.1",
    "@arcblock/did-connect": "^2.9.90",
    "@arcblock/ux": "^2.9.90",
    "@blocklet/ai-kit": "^0.1.37",
    "@blocklet/ai-runtime": "workspace:^",
    "@blocklet/dataset-sdk": "workspace:^",
    "@blocklet/discuss-kit": "^1.6.264",
    "@blocklet/logger": "^1.16.26",
    "@blocklet/pages-kit": "^0.2.340",
    "@blocklet/sdk": "^1.16.26",
    "@blocklet/ui-react": "^2.9.90",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@ocap/client": "^1.18.123",
    "@ocap/mcrypto": "^1.18.123",
    "@ocap/wallet": "^1.18.123",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.3.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.23.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.37.3",
    "snowflake-uuid": "^1.0.0",
    "sqlite3": "^5.1.7",
    "ufo": "^1.5.3",
    "umzug": "^3.8.1",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.34",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/yaml": "^1.9.7",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "vite-plugin-blocklet": "^0.7.9",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "zx": "^7.2.3"
  }
}
